//1
Program Jumlah_Bilangan
kamus 
    i, jumlah : integer
algoritma
    for i <- 1 to 100 do
        jumlah <- jumlah + i 
    endfor
    output(jumlah)
endprogram

------------------------

//2
Program Jumlah_Bilangan2
kamus 
    i, jumlah : integer
algoritma
    jumlah <- 0
    for i <- 1 to 1000 do
        jumlah <- jumlah + i 
    endfor
    output(jumlah)
endprogram

------------------------

//3
Program Jumlah_Bilangan3
kamus 
    i, jumlah : integer
algoritma
    jumlah <- 0
    for i <- 2 to 50 do
        jumlah <- jumlah + i 
    endfor
    output(jumlah)
endprogram

------------------------

//4
Program Jumlah_Bilangan_Plus2
kamus
    i, jumlah : integer
algoritma
    jumlah <- 0
    for i <- 1 to 100 do
        i <- i + 2
        jumlah <- jumlah + 1
    endfor
    output(jumlah)
endprogram

//5
Program Jumlah_Deret
kamus
    i, n, m : integer
    deret : real
algoritma
    deret <- 0
    input(n, m)
    for i <- n to m do
        deret <- deret + 4 / i
    endfor
    output(deret)
endprogram

//6
Program Jumlah_Deret2
kamus
    i, n, m : integer
    deret : real
algoritma
    deret <- 0
    input(n, m)
    for i <- n to m do
        deret <- deret + 2 / i
    endfor
    output(deret)
endprogram

